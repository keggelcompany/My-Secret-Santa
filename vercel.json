{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/dist/public/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
}